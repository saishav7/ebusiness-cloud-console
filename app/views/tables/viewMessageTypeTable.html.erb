<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Tables</title>
	  $(document).ready(function(){
	  $("#addEntry").click(function () {
	  	document.getElementById('viewEntryFields').style.display = "block";
});
});
  </script>
   </head>
<body>
    <!--  wrapper -->
    <div id="wrapper">
        <!--  page-wrapper -->
        <div id="page-wrapper">

            <div class="row">
                <!-- Page Header -->
                <div class="col-lg-12">
                    <h1 class="page-header">Tables</h1>
                </div>
                <!--End Page Header -->
            </div>
	    <!--Table-->
	            <div class="panel panel-primary">
	                <div class="panel-heading">
	                    <i class="fa fa-bar-chart-o fa-fw"></i>Message Type
	                </div>

	                <div class="panel-body">
	                    <div class="row">
	                        <div class="col-lg-12">
	                            <div class="table-responsive" style="overflow:scroll;">
	                                <table class="table table-bordered table-hover table-striped">
	                                    <thead>
	                                        <tr>
	                                            <th>Id</th>
	                                            <th>Status Id</th>
	                                            <th>Name</th>
	                                            <th>In Role</th>
	                                            <th>Out Role</th>
	                                            <th>SBDH Header Version</th>
	                                            <th>SBDH Docid Type Version</th>
	                                            <th>SBDH Docid Type</th>
	                                            <th>SBDH Docid Multi Type</th>
	                                            <th>SBDH Bizscope Type</th>
	                                            <th>SBDH Bizscope Id</th>
	                                            <th>SBDH Bizscope Instance Id</th>
	                                            <th>Trans Cmd Dch Type</th>
	                                            <th>Compliance Map</th>
	                                            <th>Pre Compliance Map</th>
	                                            <th>Post Compliance Map</th>
	                                        </tr>
	                                    </thead>
	                                    <tbody>
	                                    	<% @message_type_table.each do |msgType| %>
	                                        <tr>
	                                            <td><%= msgType.msgtype_id %></td>
	                                            <td><%= msgType.msgtype_status_id %></td>
                                                <td><%= msgType.msgtype_name %></td>  
                                                <td><%= msgType.msgtype_in_role %></td>
                                                <td><%= msgType.msgtype_out_role %></td>
                                                <td><%= msgType.msgtype_sbdh_header_ver %></td> 
                                                <td><%= msgType.msgtype_sbdh_docid_type_ver %></td>
                                                <td><%= msgType.msgtype_sbdh_docid_type %></td>
                                                <td><%= msgType.msgtype_sbdh_docid_multi_type %></td>
                                                <td><%= msgType.msgtype_sbdh_bizscope_type %></td>
                                                <td><%= msgType.msgtype_sbdh_bizscope_id %></td>
                                                <td><%= msgType.msgtype_sbdh_bizscope_instance_id %></td>
                                                <td><%= msgType.msgtype_trans_cmd_dch_type %></td>
                                                <td><%= msgType.msgtype_compliance_map %></td>
                                                <td><%= msgType.msgtype_pre_compliance_map %></td>
                                                <td><%= msgType.msgtype_post_compliance_map %></td>
	                                        </tr>	
	                                        <% end %>                                       
	                                    </tbody>
	                                </table>
	                            </div>
	                            <div>
	                             	<button id="addEntry" type="button" class="btn btn-primary">Add New Entry</button>
	                            </div>
	                            <br>
	                             <div id="viewEntryFields" style="display:none;overflow:scroll;" class="table-responsive">
                            		<input id="msgtypeId" style='width:auto' name="msgtypeId" class="form-control" placeholder="Enter Id">
	                        		<br>
                            		<input id="msgtypeStatusId" style='width:auto' name="msgtypeStatusId" class="form-control" placeholder="Enter Status Id">
	                        		<br>
                            		<input id="msgtypeName" style='width:auto' name="msgtypeName" class="form-control" placeholder="Enter Name">
	                            	<br>
                            		<input id="msgtypeInRole" style='width:auto' name="msgtypeInRole" class="form-control" placeholder="Enter In Role">
	                        		<br>
                            		<input id="msgtypeOutRole" style='width:auto' name="msgtypeOutRole" class="form-control" placeholder="Enter Out Role">
	                        		<br>
                            		<input id="msgtypeSbdhHeaderVer" style='width:auto' name="msgtypeSbdhHeaderVer" class="form-control" placeholder="Enter Sbdh Header Version">
	                        		<br>
                            		<input id="msgtypeSbdhDocidTypeVer" style='width:auto' name="msgtypeSbdhDocidTypeVer" class="form-control" placeholder="Enter SBDH Docid Type Version">
	                        		<br>
                            		<input id="msgtypeSbdhDocidType" style='width:auto' name="msgtypeSbdhDocidType" class="form-control" placeholder="Enter SBDH Docid Type">
	                        		<br>
                            		<input id="msgtypeSbdhDocidMultiType" style='width:auto' name="msgtypeSbdhDocidMultiType" class="form-control" placeholder="Enter SBDH Docid Multi Type">
	                        		<br>
                            		<input id="msgtypeBizscopeType" style='width:auto' name="msgtypeBizscopeType" class="form-control" placeholder="Enter Bizscope Type">
	                        		<br>
                            		<input id="msgtypeBizscopeId" style='width:auto' name="msgtypeBizscopeId" class="form-control" placeholder="Enter Bizscope Id">
	                        		<br>
                            		<input id="msgtypeBizscopeInstanceId" style='width:auto' name="msgtypeBizscopeInstanceId" class="form-control" placeholder="Enter Bizscope Instance Id">
	                        		<br>
                            		<input id="msgtypeTransCmdDchType" style='width:auto' name="msgtypeTransCmdDchType" class="form-control" placeholder="Enter Trans Cmd Dch Type">
	                        		<br>
                            		<input id="msgtypeComplianceMap" style='width:auto' name="msgtypeComplianceMap" class="form-control" placeholder="Enter Compliance Map">
	                        		<br>
                            		<input id="msgtypePreComplianceMap" style='width:auto' name="msgtypePreComplianceMap" class="form-control" placeholder="Enter Pre Compliance Map">
	                        		<br>
                            		<input id="msgtypePostComplianceMap" style='width:auto' name="msgtypePostComplianceMap" class="form-control" placeholder="Enter Post Compliance Map">
	                                 <br>
		                            <input id="saveEntry" value="Save" style='width:auto' type="Submit" class="btn btn-success"></input>
	                            	</form>
	                            </div>
	                        </div>
	                    </div>
	                    <!-- /.row -->
	                </div>
	                <!-- /.panel-body -->
	            </div>
	            <!--End of Table-->
        </div>
        <!-- end page-wrapper -->

    </div>
    <!-- end wrapper -->

</body>

</html>